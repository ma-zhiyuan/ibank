package org.lanqiao.bank.base.entity;

import java.util.Date;

/**@ClassName: CustomerReplay 
 * @Description: 客服回复实体类
 * @author 西安工业大学蓝桥学员-孙新召
 * @time 2017年5月7日 下午1:02:26  
 */
public class CustomerReplay {
	private Integer id;  //回复的ID
	private CustomerService customerService; //回复的客服
	private String content; //回复的内容
	private UserConsultation userConsultation; // 用户咨询
	private Date replaytime; //回复时间
	/**
	 * @return the id
	 */
	public Integer getId() {
		return id;
	}
	/**
	 * @param id the id to set
	 */
	public void setId(Integer id) {
		this.id = id;
	}
	/**
	 * @return the customerService
	 */
	public CustomerService getCustomerService() {
		return customerService;
	}
	/**
	 * @param customerService the customerService to set
	 */
	public void setCustomerService(CustomerService customerService) {
		this.customerService = customerService;
	}
	/**
	 * @return the content
	 */
	public String getContent() {
		return content;
	}
	/**
	 * @param content the content to set
	 */
	public void setContent(String content) {
		this.content = content;
	}
	/**
	 * @return the userConsultation
	 */
	public UserConsultation getUserConsultation() {
		return userConsultation;
	}
	/**
	 * @param userConsultation the userConsultation to set
	 */
	public void setUserConsultation(UserConsultation userConsultation) {
		this.userConsultation = userConsultation;
	}
	/**
	 * @return the replaytime
	 */
	public Date getReplaytime() {
		return replaytime;
	}
	/**
	 * @param replaytime the replaytime to set
	 */
	public void setReplaytime(Date replaytime) {
		this.replaytime = replaytime;
	}
	/**@Title: TODO
	 * @Description: 无参构造器
	 * @author 西安工业大学蓝桥学员-孙新召
	 * @time 2017年5月7日 下午1:31:23  
	 */
	public CustomerReplay() {
		super();
	}
	/**@Title: TODO
	 * @Description: 有参构造器
	 * @author 西安工业大学蓝桥学员-孙新召
	 * @time 2017年5月7日 下午1:31:44 
	 * @param customerService  回复的客服
	 * @param content  回复的内容
	 * @param userConsultation 回复那个咨询
	 * @param replaytime  回复时间
	 */
	public CustomerReplay(CustomerService customerService, String content, UserConsultation userConsultation,
			Date replaytime) {
		super();
		this.customerService = customerService;
		this.content = content;
		this.userConsultation = userConsultation;
		this.replaytime = replaytime;
	}
	
	@Override
	public String toString() {
		return "CustomerReplay [id=" + id + ", customerService=" + customerService + ", content=" + content
				+ ", userConsultation=" + userConsultation + ", replaytime=" + replaytime + "]";
	}
	
	
	
	
}
